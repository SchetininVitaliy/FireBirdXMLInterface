unit SQLTable;

interface
type
  TSQLTableColumn = class
    fName:string;
    fDescription:string;
    fContent: array of string;
    fType:string;
    constructor Create;
    destructor  Destroy;
  end;

  TSQLTable = class
    fTableName:string;
    fTableDescription:string;
    fContentRepresentation: array of TSQLTableColumn;
    constructor Create;
    destructor Destroy;
  end;


implementation
   constructor TSQLTable.Create;
   begin

   end;

   destructor TSQLTable.Destroy;
   begin

   end;
 ////////////////////////////////////////////////////////////
   constructor TSQLTableColumn.Create;
   begin

   end;

   destructor TSQLTableColumn.Destroy;
   begin

   end;
end.

unit SQLTable;

{$WARN SYMBOL_PLATFORM OFF}

interface

uses
  Windows, ActiveX, Classes, ComObj, FireBird2XML_TLB, StdVcl;

type
  TSQLTable = class(TTypedComObject, ISQLTable)
  private
    fTableName:string;
  protected
    function Get_TableName(out tableName: WideString): HResult; stdcall;
    function Set_TableName(const tableName: WideString): WideString; stdcall;
  end;

implementation

uses ComServ;

function TSQLTable.Get_TableName(out tableName: WideString): HResult;
begin
  StringToWideChar(Result,fTableName,Length(fTableName));
end;

function TSQLTable.Set_TableName(const tableName: WideString): WideString;
begin
   fTableName := WideCharToString(tableName);
end;

initialization
  TTypedComObjectFactory.Create(ComServer, TSQLTable, Class_SQLTable,
    ciMultiInstance, tmApartment);
end.

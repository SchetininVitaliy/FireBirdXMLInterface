unit SQLTable;

{$WARN SYMBOL_PLATFORM OFF}

interface

uses
  Windows, ActiveX, Classes, ComObj, FireBird2XML_TLB, StdVcl;

type
  TSQLTable = class(TTypedComObject, ISQLTable)
  private
    fTableName:string;
  protected
    function GetName: PAnsiChar; stdcall;
  end;

implementation

uses ComServ;

function TSQLTable.GetName: PAnsiChar;
begin
  Result := PAnsiChar(fTableName);
end;

initialization
  TTypedComObjectFactory.Create(ComServer, TSQLTable, Class_SQLTable,
    ciMultiInstance, tmApartment);
end.
